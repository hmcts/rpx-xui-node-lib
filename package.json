{
  "name": "@hmcts/rpx-xui-node-lib",
  "version": "2.26.12",
  "description": "Common Nodejs library components for XUI",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --env node",
    "test:coverage": "jest --env node --coverage",
    "build": "tsc --declarationMap --sourceMap",
    "build:watch": "tsc --declarationMap --sourceMap --watch",
    "lint": "eslint \"*/**/*.{js,ts}\" --quiet --fix",
    "commit": "git cz",
    "ci:lint": "eslint \"*/**/*.{js,ts}\" --quiet",
    "ci": "yarn && yarn ci:lint && yarn build",
    "watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hmcts/rpx-xui-node-lib.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "keywords": [
    "XUI",
    "node",
    "authentication"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hmcts/rpx-xui-node-lib/issues"
  },
  "homepage": "https://github.com/hmcts/rpx-xui-node-lib#readme",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@types/connect-redis": "^0.0.14",
    "@types/csurf": "^1.9.36",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/hapi__joi": "^17.1.0",
    "@types/jest": "^24.0.24",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^12.12.21",
    "@types/passport": "^1.0.2",
    "@types/passport-oauth2": "^1.4.9",
    "@types/session-file-store": "^1.2.1",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "commitizen": "^4.0.3",
    "commitlint-config-cz": "^0.13.1",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": ">=4",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "semantic-release": "^17.0.8",
    "standard-version": "^7.0.1",
    "supports-color": "^7.1.0",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "axios": "^0.21.1",
    "caller-path": "^3.0.0",
    "connect-redis": "^4.0.4",
    "csurf": "^1.11.0",
    "debug": "^4.1.1",
    "deepmerge": "^4.2.2",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "jest-mock-axios": "^4.2.0",
    "jest-ts-auto-mock": "^1.0.11",
    "jwt-decode": "^2.2.0",
    "openid-client": "^3.10.0",
    "otplib": "^12.0.1",
    "passport": "^0.4.1",
    "passport-oauth2": "^1.5.0",
    "redis": "^3.0.2",
    "session-file-store": "^1.3.1",
    "ts-auto-mock": "^2.2.5",
    "ttypescript": "^1.5.10"
  },
  "packageManager": "yarn@3.4.1"
}
