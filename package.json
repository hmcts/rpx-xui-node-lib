{
  "name": "@hmcts/rpx-xui-node-lib",
  "version": "1.0.1",
  "description": "Common Nodejs library components for XUI",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "npx prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "ci": "yarn format && yarn lint && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hmcts/rpx-xui-node-lib.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hmcts/rpx-xui-node-lib/issues"
  },
  "homepage": "https://github.com/hmcts/rpx-xui-node-lib#readme",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.21",
    "@types/passport": "^1.0.2",
    "@types/session-file-store": "^1.2.1",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "standard-version": "^7.0.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "openid-client": "^3.10.0",
    "passport": "^0.4.1",
    "session-file-store": "^1.3.1"
  }
}
